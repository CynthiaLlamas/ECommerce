{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "Application": {
      "Name": "ECOMOrdersService"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://${EUREKA_HOST}:8761/eureka/",
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true,
      "ShouldFetchRegistry": true
    },
    "Instance": {
      "NonSecurePort": 5261,
      "HostName": "localhost",
      "InstanceId": "ECOMOrdersService:5261"      
    }
  }
}
